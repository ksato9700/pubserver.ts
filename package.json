{
  "name": "pubserver.ts",
  "version": "0.1.1",
  "description": "Implementation of Pubserver in TypeScript",
  "main": "dist/app.js",
  "scripts": {
    "test": "tsc && ava",
    "build": "tsc",
    "postinstall": "npm run build",
    "start": "node dist/index.js",
    "dev-server": "nodemon -w src -x ts-node src/index.ts",
    "lint": "tslint src/**/*.ts test/*.ts"
  },
  "author": "Kenichi Sato <ksato9700@gmail.com>",
  "license": "MIT",
  "ava": {
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "dependencies": {
    "@koa/cors": "3.0.0",
    "axios": "0.19.0",
    "dotenv": "8.2.0",
    "iconv-lite": "0.5.0",
    "jszip": "3.2.2",
    "koa": "2.11.0",
    "koa-body": "4.1.1",
    "koa-compress": "3.0.0",
    "koa-morgan": "1.0.1",
    "koa-router": "7.4.0",
    "koa-static-server": "1.4.0",
    "mongodb": "3.4.1",
    "redis": "2.8.0"
  },
  "devDependencies": {
    "@types/jszip": "3.1.6",
    "@types/koa": "2.11.0",
    "@types/koa-compress": "2.0.9",
    "@types/koa-morgan": "1.0.4",
    "@types/koa-router": "7.0.42",
    "@types/koa-static-server": "1.3.0",
    "@types/koa__cors": "3.0.0",
    "@types/mongodb": "3.3.14",
    "@types/redis": "2.8.14",
    "@types/request-promise": "4.1.45",
    "@types/supertest": "2.0.8",
    "ava": "2.4.0",
    "supertest": "4.0.2",
    "ts-node": "8.5.4",
    "typescript": "3.7.4"
  }
}
