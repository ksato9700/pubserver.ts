{
  "name": "pubserver.ts",
  "version": "0.1.0",
  "description": "Implementation of Pubserver in TypeScript",
  "main": "dist/app.js",
  "scripts": {
    "test": "tsc && ava",
    "build": "tsc",
    "postinstall": "npm run build",
    "start": "node dist/index.js",
    "dev-server": "nodemon -w src -x ts-node src/index.ts",
    "lint": "tslint src/**/*.ts test/*.ts"
  },
  "author": "Kenichi Sato <ksato9700@gmail.com>",
  "license": "MIT",
  "ava": {
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "dependencies": {
    "@koa/cors": "2.2.2",
    "dotenv": "6.1.0",
    "iconv-lite": "0.4.24",
    "jszip": "3.1.5",
    "koa": "2.6.2",
    "koa-body": "4.0.4",
    "koa-compress": "3.0.0",
    "koa-morgan": "1.0.1",
    "koa-router": "7.4.0",
    "koa-static-server": "1.3.4",
    "mongodb": "3.1.10",
    "redis": "2.8.0",
    "request": "2.88.0",
    "request-promise": "4.2.2"
  },
  "devDependencies": {
    "@types/dotenv": "6.1.0",
    "@types/jszip": "3.1.4",
    "@types/koa": "2.0.46",
    "@types/koa-compress": "2.0.8",
    "@types/koa-morgan": "1.0.4",
    "@types/koa-router": "7.0.34",
    "@types/koa-static-server": "1.3.0",
    "@types/koa__cors": "2.2.3",
    "@types/mongodb": "3.1.14",
    "@types/redis": "2.8.8",
    "@types/request-promise": "4.1.42",
    "@types/supertest": "2.0.6",
    "ava": "1.0.0-rc.2",
    "supertest": "3.3.0",
    "ts-node": "7.0.1",
    "typescript": "3.1.6"
  }
}
